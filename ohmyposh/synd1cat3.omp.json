{
    "$schema": "https://ohmyposh.dev/schema.json",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "type": "path",
                    "style": "powerline",
                    "foreground": "#00ff00",
                    "background": "#000000",
                    "properties": {
                        "prefix": "??",
                        "style": "agnoster_short"
                    }
                },
                {
                    "type": "git",
                    "style": "powerline",
                    "foreground": "#000000",
                    "background": "#00ff00",
                    "properties": {
                        "prefix": "??",
                        "style": "compact"
                    }
                }
            ]
        }
    ]
}
